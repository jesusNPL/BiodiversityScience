@incollection{cavender-bares_applying_2020,
	abstract = {Biodiversity is organized hierarchically from individuals to populations to major lineages in the tree of life. This hierarchical structure has consequences for remote sensing of plant phenotypes and leads to the expectation that more distantly related plants will be more spectrally distinct. Applying remote sensing to understand ecological processes from biodiversity patterns builds on prior efforts that integrate functional and phylogenetic information of organisms with their environmental distributions to discern assembly processes and the rules that govern species distributions. Spectral diversity metrics critical to detecting biodiversity patterns expand on the many metrics for quantifying multiple dimensions of biodiversity---taxonomic, phylogenetic, and functional---and can be applied at local (alpha diversity) to regional (gamma diversity) scales to examine variation among communities (beta diversity). Remote-sensing technologies stand to illuminate the nature of biodiversity-ecosystem function relationships and ecosystem service trade-offs over large spatial extents and to estimate their uncertainties. Such advances will improve our capacity to manage natural resources in the Anthropocene.},
	author = {Cavender-Bares, Jeannine and Schweiger, Anna K. and Pinto-Ledezma, Jes{\'u}s N. and Meireles, Jose Eduardo},
	booktitle = {Remote Sensing of Plant Biodiversity},
	date = {2020},
	date-added = {2023-04-21 14:46:13 -0500},
	date-modified = {2023-04-21 14:46:13 -0500},
	doi = {10.1007/978-3-030-33157-3_2},
	editor = {Cavender-Bares, Jeannine and Gamon, John A. and Townsend, Philip A.},
	file = {Full Text PDF:/Users/jesusnpl/Zotero/storage/MHZYZPH2/Cavender-Bares et al. - 2020 - Applying Remote Sensing to Biodiversity Science.pdf:application/pdf},
	isbn = {978-3-030-33157-3},
