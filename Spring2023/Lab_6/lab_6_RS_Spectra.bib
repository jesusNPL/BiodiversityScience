	langid = {english},
	number = {1},
	pages = {475--505},
	shortjournal = {Annu. Rev. Ecol. Syst.},
	title = {Phylogenies and Community Ecology},
	url = {https://www.annualreviews.org/doi/10.1146/annurev.ecolsys.33.010802.150448},
	urldate = {2022-02-25},
